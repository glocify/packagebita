function getlatlong()
{
	var town=$("#city_search").val();
	var street = 'Lorem';
	if(town!="" && street!="")
	{
		$.ajax({
				type: 'post',
				url: base_url+'/Site/getLatLong',
				data: {cityid: town,street: street},
				success:function(result)
				{
					var latlong =$.trim(result).split(',');
					$("#lati").val(latlong[0]);
					$("#longi").val(latlong[1]);
				}
		});

	}else{
		alert("Please enter City and street firt!");
	}
}


