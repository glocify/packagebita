$("#p_phone,#phone").mask("(999) 999-9999");
$("#p_phone,#phone").on("blur", function() {
	var last = $(this).val().substr( $(this).val().indexOf("-") + 1 );
	if( last.length == 5 ) {
	  var move = $(this).val().substr( $(this).val().indexOf("-") + 1, 1 );
	  var lastfour = last.substr(1,4);
	  var first = $(this).val().substr( 0, 9 );
	  $(this).val( first + move + '-' + lastfour );
	}
});
function UpdateProfile(a){
		var first_name = $('#p_first_name').val();
		var last_name = $('#p_last_name').val();
		var email = $('#p_email').val();
		var address = $('#p_address').val();
		var p_phone = $('#p_phone').val();
		var p_city = $('#p_city').val();
		var about_me = $('#p_about_me').val();
		var offer = $('#p_offer').val();
		var url = $('#p_url').val();
		var Hour_rate = $('#p_Hour_rate').val();
		var speciality = $('#p_speciality').val();
		var position = $('#p_position').val();
		var company = $('#p_company').val();
		 $.ajax({
				type: 'post',
				url: base_url+'/User/UpdateProfile',
				data: {	first_name: first_name,last_name: last_name,email: email,address: address,phone: p_phone,city: p_city, about_me: about_me, offer:offer,url:url,Hour_rate:Hour_rate,speciality:speciality,position:position,company:company},
				beforeSend:function(){
				$("#up"+a).html('<img style="margin:0px 0px 0px 0px" src="'+base_url+'/images/003.gif" />');
				},
				success: function(data)
				{
					if(a=='1')
						$("#up"+a).html(first_name);
					else if(a=='2')
						$("#up"+a).html(last_name);
					else if(a=='3')
						$("#up"+a).html(email);
					else if(a=='4')
						$("#up"+a).html(address);
					else if(a=='5')
						$("#up"+a).html(p_phone);
					else if(a=='6')
						$("#up"+a).html(offer);
					else if(a=='7')
						$("#up"+a).html(about_me);
					else if(a=='8')
						$("#up"+a).html(url);
					else if(a=='9')
						$("#up"+a).html('$'+Hour_rate);
					else if(a=='10')
						$("#up"+a).html(speciality);
					else if(a=='11')
						$("#up"+a).html(position);
					else if(a=='12')
						$("#up"+a).html(company);
						
						
					$("#UpdateMsg").html('Update Saved !!');
				},
				error: function()
				{
					//alert("System Error");
				},
				});
	 }
