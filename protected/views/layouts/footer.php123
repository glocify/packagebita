function a_reportsubmit(event){
	var valid;
	valid = newvalidateras();
	if(valid){
		var ptype    = $('input[name=problem_type]:checked').val();
		var raemail  = $("#ra_email").val();
		var ra_agree = $('input[name=ra_agree]:checked').val();
		var raadsid = $('#raadsid').val();
		var ownereid  = $("#adowner_email").val();
		var ownername  = $("#adowner_name").val();
		tinyMCE.triggerSave(false, true);
		var message  = $("#ra_comments").val();
		jQuery.ajax({
			url: base_url+"/Site/ReportAds",
			data:'problem_type='+ptype+'&email='+raemail+'&ra_agree='+ra_agree+'&comments='+message+'&adsid='+raadsid+'&adowneremail='+ownereid+'&adownername='+ownername,
			type: "POST",
			success:function(data){
				$("#successmsg-ras").html(data);
				$("#successmsg-ras").css('color','green');
				$("#successmsg-ras").css('display','block');
				setTimeout(function() {
					$('#successmsg-ras').fadeOut('slow');
				}, 10000);
				document.getElementById("reportadform").reset();
			},
			error:function (){}
		});
	}
	return false;
}
function newvalidateras(){
	var valid = true;
	$('#errormsg-ras').hide();
	$('#successmsg-ras').hide();
	if ($('input[name=problem_type]:checked').length <= 0) {
		$('#errormsg-ras').html('Please Check Problem Type First');
		$('#errormsg-ras').css('display','block');
		$('#errormsg-ras').css('color','red');
		valid = false;
		return valid;
	}
	if($("#ra_email").val()=='') {
		$('#errormsg-ras').css('display','block');
		$("#errormsg-ras").html("Please Enter Email Id");
		$('#errormsg-ras').css('color','red');
		valid = false;
		return valid;

	}
	if(!$("#ra_email").val().match(/^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/)) {
		$('#errormsg-ras').css('display','block');
		$("#errormsg-ras").html("Invalid Email Id");
