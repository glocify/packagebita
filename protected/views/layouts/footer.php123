function submitfeedback(){
	var valid = true;
	$('#enqrymsg').hide();
	var Subject = $("#subject option:selected").val();
	if(Subject=='') {
		$('#enqrymsg').css('display','block');
		$("#enqrymsg").html("Please Select Your Subject!");
		$('#enqrymsg').css('color','red');
		valid = false;
		return valid;
	}
	if(!$("#emailid").val()) {
		$('#enqrymsg').css('display','block');
		$("#enqrymsg").html("Please Enter Your Email Id");
		$('#enqrymsg').css('color','red');
		valid = false;
		return valid;
	}
	if(!$("#emailid").val().match(/^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/)) {
		$('#enqrymsg').css('display','block');
		$("#enqrymsg").html("Please Enter Your Valid Email Id");
		$('#enqrymsg').css('color','red');
		valid = false;
		return valid;
	}
	if(!$("#urmessage").val()) {
		$('#enqrymsg').css('display','block');
		$("#enqrymsg").html("Please Enter Message");
		$('#enqrymsg').css('color','red');
		valid = false;
		return valid;
	}
	var Emailid = $("#emailid").val();
	var Message = $("#urmessage").val();
	$.ajax({
			type: 'POST',
			url:  base_url+'/Page/SendFeedback/',
			data: { subject : Subject,eid : Emailid,msg : Message },
			success: function(data){
				response = $.trim(data);
					$('#enqrymsg').html(response);
					$('#enqrymsg').css('display','block');
					if(response == 'There seems some error.'){
						$('#enqrymsg').css('color','red');
					} else {
						$('#enqrymsg').css('color','green');
					}
					setTimeout(function() {
						$('#enqrymsg').fadeOut('slow');
						}, 20000);
					document.getElementById("feedbackform").reset();
			},
			error: function(){
				alert('Please Try Again!');
			},
	});
	return false;
}
